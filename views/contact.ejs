  <div class="parallax-container">
    <div class="parallax aboutPara"><img src="images/parallax4.jpg"></div>
      <div class="row">
        <div class="container">
          <div class="card-panel green darken-4">
            <form class="contact" method="post" action="/send">
                  <div class="row">
                    <div class="input-field col s10 l6">
                      <i class="material-icons prefix">account_circle</i>
                      <input id="icon_prefix" type="text" class="validate name" name="name">
                      <label for="icon_prefix">Full Name</label>
                    </div>
                    <div class="input-field col s10 l6">
                      <i class="material-icons prefix">phone</i>
                      <input id="icon_telephone" name="phone" type="tel" class="validate phone">
                      <label for="icon_telephone">Telephone</label>
                    </div>
                  </div>
                  <div class="input-field col s10">
                      <i class="material-icons prefix">markunread_mailbox</i>
                      <input id="icon_prefix" type="text" class="validate email" name="mail">
                      <label for="icon_prefix">Email Adress</label>
                  </div>
                  <div class="row">
                    <div class="input-field col s10">
                      <i class="material-icons prefix" id="email">email</i>
                        <textarea id="textarea1" class="materialize-textarea message" name="comment"></textarea>
                        <label for="textarea1" id="messagelabel">Message</label>
                    </div>
                  </div> 
                   <div class="row">
                 <div class="col s2 offset-s4">
                   <button class="btn waves-effect waves-light" type="submit" value="Send">Send
                      <i class="material-icons right">send</i>
                   </button>
                 </div>
                 </div>
            </div>
                
            </form>
          </div>
        </div>
      </div>
